# Database connection settings

DB_TYPE=postgres                   # Database engine (keep as 'postgres')
DB_HOST=your-db-host.com           # Replace with your database host (e.g., localhost or remote URL)
DB_PORT=5432                       # Default PostgreSQL port
DB_USERNAME=your_db_username       # Replace with your database username
DB_PASSWORD=your_db_password       # Replace with your database password
DB_DATABASE=your_database_name     # Replace with the name of your database
DB_DEFAULT_SCHEMA=public           # Replace if using a custom PostgreSQL schema